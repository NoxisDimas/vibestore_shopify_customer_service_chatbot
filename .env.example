# =====================================================
# INTERNAL ENVIRONMENT
# =====================================================
COMPOSE_PROJECT_NAME=urban_vibe_store
ENVIRONMENT=dev
AGENT_NAME=CustomerServiceAgent
API_KEY=

# =====================================================
# LLM MODE
# static  -> pakai 1 provider tetap
# auto    -> fallback berdasarkan LLM_PRIORITY_LIST
# =====================================================
